// @flow

/** */
export type HslColor = {
  hue: number,
  saturation: number,
  lightness: number,
}

/** */
export type HslaColor = {
  hue: number,
  saturation: number,
  lightness: number,
  alpha: number,
}

/** */
export type RgbColor = {
  red: number,
  green: number,
  blue: number,
}

/** */
export type RgbaColor = {
  red: number,
  green: number,
  blue: number,
  alpha: number,
}
